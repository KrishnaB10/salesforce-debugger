<!DOCTYPE html>
<html ng-app ="SDebugger">
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script type="text/javascript" src = "/javascripts/jquery.min.js"></script>
    <script type="text/javascript" src = "/javascripts/angular.min.js"></script>
    <link href="/stylesheets/bootstrap.min.css" rel="stylesheet">
   	<script src="javascripts/bootstrap.min.js"></script>
 	<script src="/javascripts/bootstrap.min.js"></script>
 	<script src="/socket.io/socket.io.js"></script>
 	<script src="/javascripts/app.js"></script>
  </head>


    <body ng-controller="MainCtrl">
    	   	<div class="container-fluid">
				<h1>Debug Logger</h1>
				<div class="row">
					<div class="col-md-4">
						<form>
							<div class="form-group">
							<input type="email" class="form-control" id="orgId" placeholder="Org Id">
							</div>
						</form>		
					</div>
					<div class="col-md-8">
						<div class="form-group">
							<button type="submit" class="btn btn-default" id = "subscribeBtn" ng-click="subscribe()">Subscribe</button>
						</div>
					</div>
				</div>
			</div>
			<div class="container-fluid" >
				<div class="row">
					<div class="col-md-8">
						
							<div class="form-group">
							<input type="text" class="form-control" id="exampleInputEmail1" placeholder="Search" ng-model = "searchedData">
							</div>
					
					</div>
				</div>
				<div class="container-fluid" ng-repeat="log in logsData | filter:searchedData">
					<div class="row json-ctn">
						<div class="col-md-6">
							<h3>{{log.logName}}</h3>
						</div>
						<div class="col-md-3">
							<h6>5-21-2015 10:38 PM Thursday</h6>
						</div>
						<div class="col-md-1">
							<h6><span class="glyphicon glyphicon-remove crossIcon" ng-click="deleteLog($index)" title = "Delete log"></span></h6>
						</div>
					</div>	
					<div class="row">
						<div class="col-md-12">

						<code>
						 {{log.logs}}
						</code>
						</div>
					</div>
				</div>
				
			</div>

			
	</body>

	
</html>
